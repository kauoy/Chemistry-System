;AAHZ,GAnEO,EAwEP;AACNM,IAAAA,IAAI,EAAE,gBADA;AAECC,IAAAA,IAAI,EAAE,KAFP;AAGCC,IAAAA,SAAS,EAAEP;AAHZ,GAxEO,EA6EP;AACNK,IAAAA,IAAI,EAAE,WADA;AAECC,IAAAA,IAAI,EAAE,MAFP;AAGCC,IAAAA,SAAS,EAAEN;AAHZ,GA7EO,EAkFP;AACCI,IAAAA,IAAI,EAAE,iBADP;AAECC,IAAAA,IAAI,EAAE,MAFP;AAGCC,IAAAA,SAAS,EAAEL;AAHZ,GAlFO,EAuFP;AACNG,IAAAA,IAAI,EAAE,SADA;AAECC,IAAAA,IAAI,EAAE,OAFP;AAGCC,IAAAA,SAAS,EAAEJ;AAHZ,GAvFO;AAJE,CAAD,EAkGb;AACEE,EAAAA,IAAI,EAAE,QADR;AAEEC,EAAAA,IAAI,EAAE,OAFR;AAGEC,EAAAA,SAAS,EAAEvB,KAHb;AAIEyB,EAAAA,IAAI,EAAE;AAACC,IAAAA,IAAI,EAAC,EAAN;AAAUC,IAAAA,KAAK,EAAC;AAAhB;AAJR,CAlGa,EAwGb;AACEN,EAAAA,IAAI,EAAE,WADR;AAEEC,EAAAA,IAAI,EAAE,UAFR;AAGEC,EAAAA,SAAS,EAAEnB,QAHb;AAIEqB,EAAAA,IAAI,EAAE;AAACC,IAAAA,IAAI,EAAC,EAAN;AAAUC,IAAAA,KAAK,EAAC;AAAhB;AAJR,CAxGa,EA8Gb;AACEN,EAAAA,IAAI,EAAE,GADR;AAEEC,EAAAA,IAAI,EAAE,MAFR;AAGEM,EAAAA,QAAQ,EAAE;AAHZ,CA9Ga;AAkHV;AACH;AACEP,EAAAA,IAAI,EAAE,GADR;AAEEE,EAAAA,SAAS,EAAEtB;AAFb,CAnHa,CAAf,C,CAwHA;;AACA,IAAM4B,MAAM,GAAG,IAAIjC,SAAJ,CAAc;AAC3BkC,EAAAA,IAAI,EAAE,MADqB;;AAE3B;AACAV,EAAAA,MAAM,EAANA,MAH2B,CAGpB;;AAHoB,CAAd,CAAf;AAKA,IAAMW,YAAY,GAAGnC,SAAS,CAACoC,SAAV,CAAoBC,IAAzC,C,CACA;;AACArC,SAAS,CAACoC,SAAV,CAAoBC,IAApB,GAA2B,SAASA,IAAT,CAAcC,QAAd,EAAwB;AAChD,SAAOH,YAAY,CAACI,IAAb,CAAkB,IAAlB,EAAwBD,QAAxB,EAAkCE,KAAlC,CAAwC,UAAAC,GAAG;AAAA,WAAIA,GAAJ;AAAA,GAA3C,CAAP;AACF,CAFD;;AAGA,eAAeR,MAAf","sourcesContent":["import Vue from 'vue';\r\n//配置路由\r\nimport VueRouter from 'vue-router'\r\nVue.use(VueRouter);\r\n//1.创建组件\r\nimport Index from '@/views/index'\r\nimport Home from '@/views/home'\r\nimport Login from '@/views/login'\r\nimport NotFound from '@/views/404'\r\nimport UpdatePassword from '@/views/update-password'\r\nimport pay from '@/views/pay'\r\nimport register from '@/views/register'\r\nimport center from '@/views/center'\r\n    import news from '@/views/modules/news/list'\r\n    import aboutus from '@/views/modules/aboutus/list'\r\n    import discussremenshangpin from '@/views/modules/discussremenshangpin/list'\r\n    import bangzhu from '@/views/modules/bangzhu/list'\r\n    import youqinglianjie from '@/views/modules/youqinglianjie/list'\r\n    import shangjia from '@/views/modules/shangjia/list'\r\n    import chathelper from '@/views/modules/chathelper/list'\r\n    import yonghu from '@/views/modules/yonghu/list'\r\n    import shangpinfenlei from '@/views/modules/shangpinfenlei/list'\r\n    import chat from '@/views/modules/chat/list'\r\n    import remenshangpin from '@/views/modules/remenshangpin/list'\r\n    import messages from '@/views/modules/messages/list'\r\n    import orders from '@/views/modules/orders/list'\r\n    import config from '@/views/modules/config/list'\r\n\r\n\r\n//2.配置路由   注意：名字\r\nconst routes = [{\r\n    path: '/index',\r\n    name: '系统首页',\r\n    component: Index,\r\n    children: [{\r\n      // 这里不设置值，是把main作为默认页面\r\n      path: '/',\r\n      name: '系统首页',\r\n      component: Home,\r\n      meta: {icon:'', title:'center'}\r\n    }, {\r\n      path: '/updatePassword',\r\n      name: '修改密码',\r\n      component: UpdatePassword,\r\n      meta: {icon:'', title:'updatePassword'}\r\n    }, {\r\n      path: '/pay',\r\n      name: '支付',\r\n      component: pay,\r\n      meta: {icon:'', title:'pay'}\r\n    }, {\r\n      path: '/center',\r\n      name: '个人信息',\r\n      component: center,\r\n      meta: {icon:'', title:'center'}\r\n    }\r\n      ,{\r\n\tpath: '/news',\r\n        name: '商城资讯',\r\n        component: news\r\n      }\r\n      ,{\r\n\tpath: '/aboutus',\r\n        name: '关于我们',\r\n        component: aboutus\r\n      }\r\n      ,{\r\n\tpath: '/discussremenshangpin',\r\n        name: '化工品评论',\r\n        component: discussremenshangpin\r\n      }\r\n      ,{\r\n\tpath: '/bangzhu',\r\n        name: '帮助',\r\n        component: bangzhu\r\n      }\r\n      ,{\r\n\tpath: '/youqinglianjie',\r\n        name: '友情链接',\r\n        component: youqinglianjie\r\n      }\r\n      ,{\r\n\tpath: '/shangjia',\r\n        name: '商家',\r\n        component: shangjia\r\n      }\r\n      ,{\r\n\tpath: '/chathelper',\r\n        name: '智能助手',\r\n        component: chathelper\r\n      }\r\n      ,{\r\n\tpath: '/yonghu',\r\n        name: '用户',\r\n        component: yonghu\r\n      }\r\n      ,{\r\n\tpath: '/shangpinfenlei',\r\n        name: '化工品分类',\r\n        component: shangpinfenlei\r\n      }\r\n      ,{\r\n\tpath: '/chat',\r\n        name: '联系我们',\r\n        component: chat\r\n      }\r\n      ,{\r\n\tpath: '/remenshangpin',\r\n        name: '化工品',\r\n        component: remenshangpin\r\n      }\r\n      ,{\r\n\tpath: '/messages',\r\n        name: '留言交流',\r\n        component: messages\r\n      }\r\n      ,{\r\n        path: '/orders/:status',\r\n        name: '订单管理',\r\n        component: orders\r\n      }\r\n      ,{\r\n\tpath: '/config',\r\n        name: '轮播图管理',\r\n        component: config\r\n      }\r\n    ]\r\n  },\r\n  {\r\n    path: '/login',\r\n    name: 'login',\r\n    component: Login,\r\n    meta: {icon:'', title:'login'}\r\n  },\r\n  {\r\n    path: '/register',\r\n    name: 'register',\r\n    component: register,\r\n    meta: {icon:'', title:'register'}\r\n  },\r\n  {\r\n    path: '/',\r\n    name: '系统首页',\r\n    redirect: '/index'\r\n  }, /*默认跳转路由*/\r\n  {\r\n    path: '*',\r\n    component: NotFound\r\n  }\r\n]\r\n//3.实例化VueRouter  注意：名字\r\nconst router = new VueRouter({\r\n  mode: 'hash',\r\n  /*hash模式改为history*/\r\n  routes // （缩写）相当于 routes: routes\r\n})\r\nconst originalPush = VueRouter.prototype.push\r\n//修改原型对象中的push方法\r\nVueRouter.prototype.push = function push(location) {\r\n   return originalPush.call(this, location).catch(err => err)\r\n}\r\nexport default router;\r\n"]}]}